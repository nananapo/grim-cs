__assign(:=; opm 3(name value) __assign(name; value) end)

:// = opp 0(comment) end;
:$  = opm 1(fn va) fn(va) end 
:== = opm 4(va1 va2) __equal(va1; va2) end
:!= = opm 4(va1 va2) !(va1 == va2) end
:+  = opm -5(va1 va2) __add(va1; va2) end
:-  = opm -5(va1 va2) va1 + __negate(va2) end
:!  = opp 10(va) 0 == va end;



// "関数"
:print = fun(text) __put(text + "\n") end
:if = __if
:while = __while
:import = fun(fileName) __eval(__read(fileName)) end
